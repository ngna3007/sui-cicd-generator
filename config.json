{
    "project_root": ".",
    "enable_deployment": true,
    "network": "testnet",
    "branches": {
        "main": {
            "deploy": true,
            "run_tests": true
        },
        "develop": {
            "deploy": false,
            "run_tests": true
        }
    },
    "notifications": {
        "discord": {
            "enabled": false,
            "webhook_url": ""
        },
        "slack": {
            "enabled": false,
            "webhook_url": ""
        }
    },
    "gas_profiling": {
        "enabled": true,
        "threshold_warning": 1000,
        "threshold_error": 5000
    },
    "test_coverage": {
        "enabled": true,
        "minimum": 80
    }
} 